ansi:
	cs=[H[2J
	cm=[#1;#2H
	c0=[0m
	c1=[1m
	c2=[7m
	c4=[0;37m
	c5=[1;34m
	c6=[0;36m
	c7=[0;31m
	c8=[0;32m
	c9=[1;36m

vt100:
	cs=[H[2J
	cm=[#1;#2H
	c0=[0m
	c1=[1m
	c2=[7m
	c4=[1m
	c5=[1m
	c6=[1m
	c7=[1m
	c8=[1m
	c9=[1m

mono-vt100:
	cs=[H[2J
	cm=[#1;#2H
	c0=[0m
	c1=[4m
	c2=[7m

debug:
	cs=$cs
	cm=$cm(#1,#2)
	c0=$c0
	c1=$c1
	c2=$c2
	c3=$c3
	c4=$c4
	c5=$c5
	c6=$c6
	c7=$c7
	c8=$c8
	c9=$c9

other:
