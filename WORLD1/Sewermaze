  }
  flags { dark, indoors }
  exits {
	to { s, 2 }
	  info { door, closed }
	  keywords { door wooden" }
	  desc { You look back through the door into the lair.
}
	to { w, 0 }
	  key { 7205 }
	  info { door, closed }
	  keywords { door stone" }
	  desc { You see the treasury.
}
  }
}

2
{
  name { The Lair }
  desc {
